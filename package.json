{
  "name": "leean-angular",
  "private": true,
  "version": "0.1.0",
  "description": "An atomic starter project for Leean",
  "repository": "https://github.com/moxie-leean/leean-angular",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "bower": "^1.3.1",
    "browserify": "^13.0.0",
    "browserify-shim": "^3.8.12",
    "eslint-plugin-angular": "^0.15.0",
    "gulp": "^3.9.0",
    "gulp-angular-templatecache": "^1.8.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-cssnano": "^2.1.0",
    "gulp-env": "^0.4.0",
    "gulp-eslint": "^1.1.1",
    "gulp-notify": "^2.0.0",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^2.1.1",
    "gulp-sass-lint": "^1.1.1",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.0.0",
    "gulp-util": "^3.0.7",
    "http-server": "^0.6.1",
    "json-server": "^0.8.8",
    "shelljs": "^0.2.6"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jQuery": "global:jQuery",
    "jQuery": "global:jQuery",
    "angular": "global:angular"
  },
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000 -c-1",
    "pretest": "npm install",
    "json-server": "json-server --watch config/json/db.json",
    "browserify": "browserify ./app/app.js -o public/build/js/production.js"
  }
}
